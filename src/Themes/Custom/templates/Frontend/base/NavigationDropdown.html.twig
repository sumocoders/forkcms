{% if navigation %}
  {% for nav in navigation %}
    <li>
      <a class="dropdown-item{% if nav.selected %} active{% endif %}" href="{{ nav.link }}" title="{{ nav.navigation_title }}"{% if nav.nofollow %} rel="nofollow"{% endif %}>{{ nav.navigation_title|e('html') }}</a>
    </li>
  {% endfor %}
{% endif %}
